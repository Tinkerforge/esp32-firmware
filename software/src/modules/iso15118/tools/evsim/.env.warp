# WARP4 Charger ISO 15118 configuration
# Copy this file to .env and adjust NETWORK_INTERFACE as needed

# Network interface connected to the same network as the WARP charger
# Use 'ip link' or 'ifconfig' to find your interface name
NETWORK_INTERFACE=enp34s0

# Path to EVCC configuration (relative to iso15118 directory)
EVCC_CONFIG_PATH=../evcc_config_warp.json

# Logging
LOG_LEVEL=DEBUG

# TLS Settings (disabled for debug mode)
SECC_ENFORCE_TLS=False
